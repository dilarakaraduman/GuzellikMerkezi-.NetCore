@model NETCore_Demo_StajProject.DAL.PersonelUzmanlik
@{
    Layout="_LayoutAdmin";
}
<form asp-controller="PersonelUzmanlik" asp-action="Create" method="post">

    @Html.HiddenFor(m=>m.PersonelId)

    <div style="height:320px">
        <table style="width:40%;margin-top:200px;background-color:#f6e1c5">

            <tr>
              <th> Uzmanlık Ad:</th> 
                  <td>        
                    @Html.DropDownList("UzmanlikId",null,"Seçiniz", new {@class="form-control" })
            
                  </td>
            </tr>
        </table>
        <br /> 
        <button type="submit" style="background-color:#f6e1c5" class="btn btn-primary">Kaydet</button>

    </div>
</form>

